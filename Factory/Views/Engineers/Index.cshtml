@{
  Layout = "_Layout";
}
@using Factory.Models;
<div class="container">
  <div class="jumbotron">
    <h3>Engineers at the Factory</h3>
  </div>
  
  @if(Model.Count == 0)
  {
    <h6>There are no Engineers at this time.</h6>
  }
  <ol>
    @foreach(Engineer engineer in Model)
    {
      <li>@Html.ActionLink(engineer.Name,"Details", new {id = engineer.EngineerId})</li>
    }
  </ol>

 
  <button class="btn btn-outline-dark" onclick="window.location.href='/Engineers/Create/'">Create and Engineer</button>
   <p>@Html.ActionLink("Home","Index","Home")</p>
  



</div>